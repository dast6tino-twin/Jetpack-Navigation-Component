<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_left"
    app:startDestination="@id/leftFragment">

    <fragment
        android:id="@+id/leftFragment"
        android:name="ru.dast_6_tino.androidx_navigation_example.main.left.LeftFragment"
        android:label="fragment_left"
        tools:layout="@layout/fragment_left">
        <action
            android:id="@+id/action_leftFragment_to_leftFragment"
            app:destination="@id/leftFragment" />
        <argument
            android:name="count"
            android:defaultValue="0"
            app:argType="integer" />
    </fragment>
</navigation>
